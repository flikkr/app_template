  "version": "0.2.0",
  "configurations": [
    {
      "name": "widgetbook",
      "type": "dart",
      "request": "launch",
      "program": "lib/widgetbook.dart",
      "args": ["-d", "macos"],
      "toolArgs": ["-t"]
    },
    {
      "name": "app",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      //   "args": ["-d", "macos"],
      "toolArgs": ["-t"]
    },
    {
      "name": "app (profile mode)",
      "type": "dart",
      "request": "launch",
      "flutterMode": "profile"
    },
    {
      "name": "app (release mode)",
      "type": "dart",
      "request": "launch",
      "flutterMode": "release"
    }
